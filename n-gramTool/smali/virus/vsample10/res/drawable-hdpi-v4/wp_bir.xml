<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffff9326" android:endColor="#ffff9326" android:angle="270.0" />
            <corners android:radius="@dimen/bbuton_rounded_corner_radius" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <gradient android:startColor="#ffff9b00" android:endColor="#ffff9b00" android:angle="270.0" />
            <corners android:radius="@dimen/bbuton_rounded_corner_radius" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ffff9b00" android:endColor="#ffff9b00" android:angle="270.0" />
            <corners android:radius="@dimen/bbuton_rounded_corner_radius" />
        </shape>
    </item>
</selector>